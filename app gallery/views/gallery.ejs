<% include partials\header%>





<div class="container">
  
    <div class="jumbotron col-lg-12 col-md-12 col-sm-12">
        
       <!--<img id="imgheader" src="http://photography-cameras.org/images/stories/wide-angle-lens.jpg" >-->
    </div>


<div class="row">
<%photos.forEach(function (photo){%>

    <div class="col-lg-3 col-md-4 col-sm-12"  style="padding:5px 5px 10px 5px">
      
        <div class="card">

              <div class="card-body">  <a href="/gallery/<%= photo._id  %>">   
                          <img src="<%= photo.image %>" id="imgcard" class="card-img-top" >
                           <h4 class="text-center">
                               
                                     <%=photo.name %></a>
                                    
                                    </h4>
               
                                 
                      
                 
              </div>
           

              
        </div>   
      
     
    </div>

<%})%>

</div>
</div>


<% include partials\footer%>